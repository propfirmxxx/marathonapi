    "@nestjs/swagger": "^11.1.5",
    "@nestjs/throttler": "^6.4.0",
    "@nestjs/typeorm": "^11.0.0",
    "@nestjs/websockets": "^11.0.1",
    "@types/cookie-parser": "^1.4.8",
    "amqplib": "^0.10.4",
    "axios": "^1.9.0",
