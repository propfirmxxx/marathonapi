{
  "rabbit_version": "4.0.0",
  "rabbitmq_version": "4.0.0",
  "product_name": "RabbitMQ",
  "product_version": "4.0.0",
  "users": [],
  "vhosts": [
    {
      "name": "/"
    }
  ],
  "permissions": [],
  "topic_permissions": [],
  "parameters": [],
  "global_parameters": [
    {
      "name": "internal_cluster_id",
      "value": "marathon-rabbitmq-cluster"
    }
  ],
  "policies": [
    {
      "vhost": "/",
      "name": "marathon-ha-policy",
      "pattern": "marathon.*",
      "apply-to": "queues",
      "definition": {
        "ha-mode": "all",
        "ha-sync-mode": "automatic",
        "message-ttl": 60000,
        "max-length": 10000,
        "overflow": "reject-publish"
      },
      "priority": 10
    },
    {
      "vhost": "/",
      "name": "dlx-policy",
      "pattern": "marathon_dead_letter",
      "apply-to": "queues",
      "definition": {
        "max-length": 50000,
        "overflow": "drop-head"
      },
      "priority": 5
    }
  ],
  "queues": [],
  "exchanges": [
    {
      "name": "marathon.live.exchange",
      "vhost": "/",
      "type": "topic",
      "durable": true,
      "auto_delete": false,
      "internal": false,
      "arguments": {}
    },
    {
      "name": "marathon.dlx",
      "vhost": "/",
      "type": "topic",
      "durable": true,
      "auto_delete": false,
      "internal": false,
      "arguments": {}
    },
    {
      "name": "marathon.retry.exchange",
      "vhost": "/",
      "type": "topic",
      "durable": true,
      "auto_delete": false,
      "internal": false,
      "arguments": {}
    }
  ],
  "bindings": []
}

